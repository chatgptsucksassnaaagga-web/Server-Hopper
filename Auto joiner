local gui = loadstring(game:HttpGet("https://pastebin.com/raw/REPLACE_WITH_YOUR_G2L"))()
-- ↑ If you already pasted G2L directly, REMOVE this line and keep gui reference below

local ScreenGui = gui
local Whole = ScreenGui:FindFirstChild("Whole frame", true)
local LogFrame = Whole:FindFirstChild("Log frame", true)

local RefreshBtn = Whole:FindFirstChild("Refresh Button", true)
local ClearBtn = Whole:FindFirstChild("TextButton", true)
local AutoJoinBtn = Whole:FindFirstChild("Auto join", true)

local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")
local HttpService = game:GetService("HttpService")
local Player = Players.LocalPlayer

local request = (syn and syn.request) or (http and http.request) or (fluxus and fluxus.request) or (krnl and krnl.request) or request
if not request then return end

local RELAY_SERVERS = "https://acne-variations-flexibility-designs.trycloudflare.com/servers"

local autoJoin = false
local logs = {}

-- SCROLLING
local layout = Instance.new("UIListLayout", LogFrame)
layout.Padding = UDim.new(0,6)

local padding = Instance.new("UIPadding", LogFrame)
padding.PaddingTop = UDim.new(0,8)
padding.PaddingLeft = UDim.new(0,6)

LogFrame.AutomaticCanvasSize = Enum.AutomaticSize.None
LogFrame.CanvasSize = UDim2.new(0,0,0,0)
LogFrame.ScrollBarImageTransparency = 0.4
LogFrame.ScrollBarThickness = 4

-- CLEAR LOGS
local function clearLogs()
	for _,v in ipairs(LogFrame:GetChildren()) do
		if v:IsA("Frame") and v.Name == "LOG" then
			v:Destroy()
		end
	end
end

-- CREATE LOG ENTRY
local function createLog(data)
	local log = Whole.Log_frame.LOG:Clone()
	log.Parent = LogFrame
	log.Visible = true

	log:FindFirstChild("Brainrot name").Text = data.brainrot
	log:FindFirstChild("MPS").Text = tostring(math.floor(data.mps/1e6)).."M"

	local join = log:FindFirstChild("Join button")
	join.MouseButton1Click:Connect(function()
		TeleportService:TeleportToPlaceInstance(
			data.placeId,
			data.jobId,
			Player
		)
	end)

	LogFrame.CanvasSize = UDim2.new(0,0,0,layout.AbsoluteContentSize.Y + 20)
end

-- FETCH SERVERS
local function fetchServers()
	local res = request({
		Url = RELAY_SERVERS,
		Method = "GET"
	})
	if not res or not res.Body then return end

	local data = HttpService:JSONDecode(res.Body)
	clearLogs()

	for _,server in ipairs(data) do
		createLog(server)
	end
end

-- AUTO JOIN LOOP
task.spawn(function()
	while true do
		task.wait(2)
		if autoJoin then
			local res = request({Url = RELAY_SERVERS, Method = "GET"})
			if res and res.Body then
				local data = HttpService:JSONDecode(res.Body)
				if data[1] then
					TeleportService:TeleportToPlaceInstance(
						data[1].placeId,
						data[1].jobId,
						Player
					)
				end
			end
		end
	end
end)

-- BUTTON HOOKS
RefreshBtn.MouseButton1Click:Connect(fetchServers)

ClearBtn.MouseButton1Click:Connect(clearLogs)

AutoJoinBtn.MouseButton1Click:Connect(function()
	autoJoin = not autoJoin
	AutoJoinBtn.Text = autoJoin and "AUTO JOIN ✓" or "AUTO JOIN"
end)
